@model IEnumerable<BoardCockpit.Models.FinancialData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Context.XbrlContextID)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Unit.XbrlUnitID)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Precision)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.XbrlName)
        </th>
        <th style="text-align:left">
            Aktionen
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Context.XbrlContextID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Unit.XbrlUnitID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Precision)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.XbrlName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FinancialDataID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FinancialDataID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FinancialDataID })
        </td>
    </tr>
}

</table>
