@model IEnumerable<BoardCockpit.Models.Context>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Company.Name)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.XbrlContextID)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Instant)
        </th>
        <th style="text-align:left">
            Aktionen
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Company.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.XbrlContextID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Instant)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContextID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ContextID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContextID })
        </td>
    </tr>
}

</table>
