@model IEnumerable<BoardCockpit.Models.Import>

@{
    ViewBag.Title = "Index";
}

<h2>
    Index
</h2>

<p>
    @Html.ActionLink("Upload New", "Upload")
</p>
<table class="table">
    <tr>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th style="text-align:left">
            @Html.DisplayNameFor(model => model.Error)
        </th>
        <th style="text-align:left">
            Aktionen
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Error)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ImportID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ImportID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ImportID })
            </td>
        </tr>
    }

</table>
